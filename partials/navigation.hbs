{{#unless isSecondary}}
    {{#foreach navigation}}
        <a class="menu-item menu-item-{{slug}}{{#if current}} menu-item-current{{/if}}" href="{{url absolute="true"}}">{{label}}</a>
    {{/foreach}}
    <a href="#/search" style="cursor: pointer;" class="menu-item menu-item-current" >{{> "icons/search"}}</a>
{{else}}
    {{#foreach navigation}}
        <a class="menu-item menu-item-{{slug}}{{#if current}} menu-item-current{{/if}}" href="{{url absolute="true"}}">{{label}}</a>
    {{/foreach}}
    <a href="#/search" style="cursor: pointer;" class="menu-item menu-item-current" >{{> "icons/search"}}</a>
{{/unless}}